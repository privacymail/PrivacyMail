    "scripts": {
        "start": "HTTPS=true react-scripts start --host 0.0.0.0",
        "build": "react-scripts build",
        "buildAndServe": "react-scripts build && serve -s build ",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
