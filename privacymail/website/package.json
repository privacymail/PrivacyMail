        "prettier": "^1.19.1",
        "react-codemod": "^5.2.0",
        "selenium-webdriver": "^4.0.0-alpha.7",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^6.2.2",
        "typescript": "^3.8.3",
        "node-fetch": "^2.6.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "prettier --write 'src/**/*.js' 'src/**/*.json' 'src/**/*.ts' 'src/**/*.tsx'"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
